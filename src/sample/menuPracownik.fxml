<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane fx:id="pracownikPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuPracownikController">
   <children>
      <BorderPane prefHeight="30.0" prefWidth="700.0">
         <top>
            <MenuBar prefHeight="25.0" prefWidth="700.0" BorderPane.alignment="CENTER">
               <menus>
                  <Menu mnemonicParsing="false" text="Powrót">
                     <items>
                        <MenuItem fx:id="logout_btn" mnemonicParsing="false" onAction="#logOut" text="Do logowania" />
                        <MenuItem fx:id="menuPracownik_btn" mnemonicParsing="false" onAction="#menuPracownik" text="Panel pracownik" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
         </top>
         <bottom>
            <AnchorPane prefHeight="132.0" prefWidth="700.0" style="-fx-background-color: #6d0303;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="zarzadzajPojazdami_btn" layoutX="177.0" layoutY="51.0" mnemonicParsing="false" onAction="#zarzadzajPojazdami" prefHeight="25.0" prefWidth="162.0" text="Zarządzaj pojazdami" translateX="7.0" />
                  <Button fx:id="zarzadzajUzytkownikami_btn" layoutX="179.0" layoutY="14.0" mnemonicParsing="false" onAction="#zarzadzajUzytkownikami" prefHeight="25.0" prefWidth="162.0" text="Zarządzaj użytkownikami" translateX="5.0" />
                  <Button fx:id="zarzadzajWypozyczeniami_btn" layoutX="179.0" layoutY="87.0" mnemonicParsing="false" onAction="#zarzadzajWypozyczeniami" prefHeight="25.0" prefWidth="162.0" text="Zarządzaj wypożyczeniami" translateX="5.0" />
                  <Button layoutX="350.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="162.0" text="Wypożycz Samochód" translateX="5.0" />
                  <Button fx:id="udostepnijPojazd" layoutX="355.0" layoutY="51.0" mnemonicParsing="false" onAction="#udostepnijPojazd" prefHeight="25.0" prefWidth="162.0" text="Udostępnij samochód" />
                  <Button fx:id="historiaWypozyczen" layoutX="355.0" layoutY="87.0" mnemonicParsing="false" onAction="#historiaWypozyczen" prefHeight="25.0" prefWidth="162.0" text="Historia wypożyczeń" />
               </children>
            </AnchorPane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
